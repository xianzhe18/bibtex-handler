{
    "name": "xianzhe18/bibtex-handler",
    "description": "BibTex Parser provides an API to read .bib files programmatically (forked from renanbr/bibtex-parser)",
    "keywords": ["bib", "bibtex", "latex", "parser", "bibliography", "citation", "cite"],
    "type": "library",
    "license": "MIT",
    "authors": [{
        "name": "Renan de Lima Barbosa, Wang Xianzhe",
        "email": "renandelima@gmail.com, xianzhe_wang@yahoo.com"
    }],
    "autoload": {
        "psr-4": {
            "RenanBr\\BibTexParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RenanBr\\BibTexParser\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.0",
        "ryakad/pandoc-php": "^1.0"
    },
    "suggest": {
        "ryakad/pandoc-php": "Needed to support LaTeX decoder in class RenanBr\\BibTexParser\\Processor\\LatexToUnicodeProcessor"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    }
}
